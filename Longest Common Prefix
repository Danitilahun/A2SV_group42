class Solution:
    def longestCommonPrefix(self, strs: List[str]) -> str:
        ans=""
        for i in range(len(strs)-1):
            for k in range(len(strs[i])):
                first=strs[0][i]
                for j in range(len(strs[i+1])):
                    if strs[i+1][j]!=first:return ans
                ans+=first
        return ans
