def insertionSort1(n, arr):
    # Write your code here
    pos=n-1
    current_value=arr[pos]
    while current_value<arr[pos-1] and pos>0:
        arr[pos]=arr[pos-1]
        pos-=1
        for i in range(len(arr)):
            print(arr[i],end=" ")
        print()
    arr[pos]=current_value
    for i in range(len(arr)):
            print(arr[i],end=" ")
